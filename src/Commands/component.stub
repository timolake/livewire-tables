<?php

namespace [namespace];

use timolake\LivewireTables\LivewireModelTable;
use Livewire\WithPagination;
use App\User;

class [class] extends LivewireModelTable
{
    use WithPagination;

    public $paginate = true;
    public $hasSearch = true;

    public $fields = [
        [
            'title' => 'id',
            'name' => 'id',
            'header_class' => '',
            'cell_class' => '',
            'sortable' => true,
            'searchable' => true,
        ]
    ];

    public function render()
    {
        return view('[view]', [
            'rowData' => $this->query(),
        ]);
    }

    public function model()
    {
        return Model::class;
    }
}
